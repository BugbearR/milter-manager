#! /bin/sh

set -e

if [ "$1" = "purge" ]; then
        rm -f /var/run/milter-manager/*
        dpkg-statoverride --remove /var/run/milter-manager
fi

# Remove the file installed by 1.8.6 or earlier
if test -f /etc/cron.d/milter-manager; then
        rm -f /etc/cron.d/milter-manager
fi

#DEBHELPER#
